{
  "name": "vistral-mvp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rm -rf .next && next dev",
    "kill:port": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true",
    "dev:force": "npm run kill:port && sleep 1 && npm run dev",
    "dev:staging": "next dev --env-file=.env.staging",
    "build": "next build",
    "build:staging": "next build --env-file=.env.staging",
    "build:prod": "next build --env-file=.env.production",
    "start": "next start",
    "start:staging": "next start --env-file=.env.staging",
    "start:prod": "next start --env-file=.env.production",
    "lint": "eslint",
    "test": "vitest",
    "env:dev": "cp .env.example .env.local && echo '✅ Created .env.local - Please fill in your development credentials'",
    "env:staging": "cp .env.example .env.staging && echo '✅ Created .env.staging - Please fill in your staging credentials'",
    "env:prod": "cp .env.example .env.production && echo '✅ Created .env.production - Please fill in your production credentials'",
    "migrate:show": "node scripts/migrate-simple.js",
    "migrate:dev": "node scripts/migrate-simple.js",
    "setup:airtable-webhook": "tsx scripts/setup-airtable-webhook.ts",
    "test:sync-airtable": "tsx scripts/test-airtable-sync.ts",
    "sync:all-phases": "tsx scripts/sync-all-phases.ts",
    "sync:initial-check": "tsx scripts/sync-initial-check.ts",
    "sync:upcoming-settlements": "tsx scripts/sync-upcoming-settlements.ts",
    "sync:reno-in-progress": "tsx scripts/sync-reno-in-progress.ts",
    "migrate:upcoming-to-initial-check": "tsx scripts/migrate-upcoming-to-initial-check.ts",
    "sync:upcoming-reno-budget": "tsx scripts/sync-upcoming-reno-budget.ts",
    "sync:furnishing-cleaning": "tsx scripts/sync-furnishing-cleaning.ts",
    "sync:final-check": "tsx scripts/sync-final-check.ts",
    "sync:roles-to-auth0": "tsx scripts/sync-roles-to-auth0.ts",
    "check:migrations": "tsx scripts/check-migrations-status.ts",
    "verify:initial-check": "tsx scripts/verify-initial-check-properties.ts",
    "check:properties-status": "tsx scripts/check-properties-status.ts",
    "verify:synced-properties": "tsx scripts/verify-synced-properties.ts",
    "debug:airtable-fields": "tsx scripts/debug-airtable-fields.ts",
    "verify:fields-in-db": "tsx scripts/verify-fields-in-db.ts",
    "list:airtable-tables": "tsx scripts/list-airtable-tables.ts",
    "debug:related-tables": "tsx scripts/debug-related-tables.ts",
    "check:supabase-columns": "tsx scripts/check-supabase-columns.ts",
    "find:missing-fields": "tsx scripts/find-missing-fields.ts",
    "clear:properties": "tsx scripts/clear-properties-table.ts",
    "verify:table-empty": "tsx scripts/verify-table-empty.ts"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.9.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.83.0",
    "airtable": "^0.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^0.6.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.548.0",
    "next": "16.0.1",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.65.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv": "^16.6.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "jsdom": "^24.0.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^1.6.0"
  }
}
